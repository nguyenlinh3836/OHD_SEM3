@model IEnumerable<Request>
<h1>List Request</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Description</th>
            <th>User</th>
            <th>Status</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var request in Model)
        {
            <tr>
                <td>@request.Subject</td>
                <td>@request.Description</td>               
                <td>@request.Status</td>   
                <td>

                </td>
            </tr>
        }
    </tbody>
</table>
